{
  "name": "NightSky",
  "displayName": "NightSky Theme",
  "version": "1.0.0",
  "publisher": "Abeilles",
  "description": "night theme",
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Themes"
  ],
  "icon": "images/icon.png",
  "license": "https://github.com/Abeilles8/NightSky/blob/main/LICENSE",
  "homepage": "https://github.com/Abeilles8/NightSky",
  "bugs": {
    "url": "https://github.com/Abeilles8/NightSky/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Abeilles8/NightSky.git"
  },
  "activationEvents": [
    "onCommand:NightSky.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "themes": [
      {
        "label": "NightSky",
        "uiTheme": "vs-dark",
        "path": "./theme/night-sky-color-theme.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.54.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}
